        System.out.println(fullName);
    }
}

public class first {
    public static void main(String[] args) {

        int age = 22;
        double myDouble = age;

        System.out.println(age);
        System.out.println(myDouble);
    }
}

public class first {
    public static void main(String[] args) {

        double myDouble = 9.987f;
        int age = (int) myDouble;

        System.out.println(myDouble);
        System.out.println(age);
    }
}

